<schemalist>
  <schema gettext-domain="@GETTEXT_PACKAGE@" id="org.gnome.liferea" path="/org/gnome/liferea/">
    <child name="plugins" schema="org.gnome.liferea.plugins"/>
  </schema>

  <schema gettext-domain="@GETTEXT_PACKAGE@" id="org.gnome.liferea.plugins" path="/org/gnome/liferea/plugins/">
    <key name="active-plugins" type="as">
      <default>['translation-memory']</default>
      <summary>Active plugins</summary>
      <description>List of active plugins. It contains the "Location" of the active plugins. See the .liferea-plugin file for obtaining the "Location" of a given plugin.</description>
    </key>
  </schema>
</schemalist>
